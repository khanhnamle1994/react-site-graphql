{
	"name": "babel-preset-codogo",
	"version": "1.1.1",
	"main": "index.js",
	"repository": "git@bitbucket.org:codogo/babel-preset-codogo.git",
	"author": "hootiebrown@codogo.io",
	"config": {
		"validate-commit-msg": {
			"types": [
				"feat",
				"fix",
				"docs",
				"style",
				"refactor",
				"perf",
				"merge",
				"test",
				"chore",
				"revert"
			],
			"warnOnFail": false,
			"maxSubjectLength": 100,
			"subjectPattern": ".+",
			"subjectPatternErrorMsg": "subject does not match subject pattern!",
			"helpMessage": "Valid commit types are: merge feat, fix, docs, style, refactor, perf, test, chore, revert"
		}
	},
	"scripts": {
		"prepush": "npm run format",
		"commitmsg": "validate-commit-msg",
		"postmerge": "yarn install",
		"postrewrite": "yarn install",
		"format": "eslint --fix index.js",
		"lint": "eslint index.js"
	},
	"license": "MIT",
	"eslintConfig": {
		"extends": "codogo"
	},
	"devDependencies": {
		"eslint": "^3.19.0",
		"eslint-config-codogo": "^1.1.4",
		"husky": "^0.13.4",
		"validate-commit-msg": "^2.12.1"
	},
	"dependencies": {
		"babel-plugin-inline-react-svg": "^0.4.0",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-export-extensions": "^6.22.0",
		"babel-plugin-transform-regenerator": "^6.24.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-react-app": "^2.2.0",
		"babel-runtime": "^6.23.0"
	}
}
